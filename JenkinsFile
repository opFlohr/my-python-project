pipeline { 
agent any 
stages { 
  stage('Checkout') {
    steps {
          git 'https://github.com/BAliani/my-python-project.git'
         }
  }
stage('Flake8') { 
steps { 
 echo 'Executing Flake8.' 
 sh 'python3 --version'
} 
} 
stage('Test') { 
steps { 
echo 'Testing...' 
} 
} 
stage('Deploy') { 
steps { 
echo 'Deploying...' 
} 
} 
} 
} 
